{
  "name": "in-square/pimcore-simple-search-bundle",
  "description": "MySQL FULLTEXT based frontend search index for Pimcore (Documents + DataObjects).",
  "type": "pimcore-bundle",
  "keywords": ["pimcore", "search", "pimcore-bundle", "pimcore-plugin"],
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/in-square/pimcore-simple-search-bundle",
  "authors": [
    {
      "name": "Władysław Dudko",
      "email": "w.dudko@insquare.pl",
      "homepage": "https://insquare.pl/"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/console": "^6.4",
    "doctrine/dbal": "^3.6 || ^4.0",
    "pimcore/pimcore": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "InSquare\\PimcoreSimpleSearchBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "InSquare\\PimcoreSimpleSearchBundle"
      ]
    }
  }
}